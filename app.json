{
  "name": "boilerplate-angular",
  "scripts": {},
  "env": {
    "NODE_ENV": {
      "description": "Node environment. node_modules packages can alter their behaviour when in production. Heroku will prune devPackages when in production.",
      "value": "production"
    },
    "NODE_CONFIG_ENV": {
      "description": "Realm for loading config entries.",
      "required": true
    },
    "contractAddress": {
      "required": true
    },
    "ropstenURL": {
      "required": true
    },
    "apiEndpoint": {
      "required": true
    }
  },
  "formation": {
    "npm": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "addons": [],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
